                    {
                      "id": "_35u057pgd",
                      "parentId": "_3481232sd",
                      "title": "ComValue.SetValueFromInvokeMethod",
                      "slug": "Utility-Classes/Class-wwDotnetBridge/NET-Wrapper-Types-and-Value-Fixups/Class-ComValue/ComValue-SetValueFromInvokeMethod",
                      "link": "Utility-Classes/Class-wwDotnetBridge/NET-Wrapper-Types-and-Value-Fixups/Class-ComValue/ComValue-SetValueFromInvokeMethod.md",
