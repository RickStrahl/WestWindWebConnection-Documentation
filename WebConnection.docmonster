                {
                  "id": "_3ph118vs4",
                  "parentId": "_7030oqlek",
                  "title": "wwDotNetBridge::FromXml",
                  "slug": "Utility-Classes/Class-wwDotnetBridge/Class-wwDotnetBridge-Support-Functions/wwDotNetBridgeFromXml",
                  "link": "Utility-Classes/Class-wwDotnetBridge/Class-wwDotnetBridge-Support-Functions/wwDotNetBridgeFromXml.md",
